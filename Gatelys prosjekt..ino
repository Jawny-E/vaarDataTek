  sensorValue = analogRead(sensorPin);
  if (sensorValue < Grenseverdi1 & (millis() >= period)) {  
    display.clearDisplay();   // fjerne det som ble skrevet sist fra natt mÃ¥linga
    display.setCursor(0,0);
    digitalWrite(ledPin13, HIGH);     // gatelys 4 stk
    digitalWrite(ledPin12, HIGH); 
    digitalWrite(ledPin11, HIGH);
    digitalWrite(ledPin10, HIGH);
    Serial.println(sensorValue);
    display.drawBitmap(0, 0, image_data_natt, 128, 64, 1);
    display.display();
  }
  if (sensorValue > Grenseverdi1 & (millis() >= period)) {
    display.clearDisplay();  // Fjerne natt mÃ¥linga
